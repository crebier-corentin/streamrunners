    "@types/chai": "^4.1.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.35",
    "@types/cssnano": "^4.0.0",
    "@types/debug": "^0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/eslint": "^4.16.3",
    "@types/express": "^4.16.0",
    "@types/jquery": "^3.3.6",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.35",
    "@types/nunjucks": "^3.1.0",
    "@types/passport": "^0.4.6",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sqlite3": "^3.1.3",
    "@types/typescript": "^2.0.0",
    "@types/vue": "^2.0.0",
    "braintree": "^2.12.0",
    "canvas": "^2.6.1",
    "chai-as-promised": "^7.1.1",
