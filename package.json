{
  "name": "twitch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all --parallel build:sass build:js",
    "build:js": "vuetype asset/js/component/ && webpack --mode=production",
    "build:js:dev": "vuetype asset/js/component/ && webpack --mode=development -w",
    "build:sass": "gulp sass",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "start": "node --require ts-node/register bin/www",
    "test": "mocha -R spec --ui bdd -r ts-node/register --require test/mocha.env.js --timeout 15000 \"test/**/*.test.ts\"",
    "forever:start": "forever -c \"node --require ts-node/register\" start bin/www",
    "forever:stop": "forever stopall"
  },
  "dependencies": {
    "@types/autoprefixer": "^6.7.3",
    "@types/axios": "^0.14.0",
    "@types/bootstrap": "^4.1.2",
    "@types/chai": "^4.1.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/cookie-session": "^2.0.35",
    "@types/cssnano": "^4.0.0",
    "@types/debug": "^0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/eslint": "^4.16.3",
    "@types/express": "^4.16.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-sass": "^0.0.31",
    "@types/jquery": "^3.3.6",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.7.35",
    "@types/nunjucks": "^3.1.0",
    "@types/passport": "^0.4.6",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sqlite3": "^3.1.3",
    "@types/typescript": "^2.0.0",
    "@types/vue": "^2.0.0",
    "@types/webpack": "^4.4.11",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-nunjucks": "^2.2.3",
    "helmet": "^3.13.0",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "nunjucks": "^3.1.3",
    "passport": "^0.4.0",
    "passport-twitch": "^1.0.3",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^4.0.2",
    "ts-node": "^7.0.1",
    "typeorm": "^0.3.0-alpha.16",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "autoprefixer": "^9.1.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bootstrap.native": "^2.0.23",
    "chai": "^4.1.2",
    "createjs-module": "^0.8.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.0.5",
